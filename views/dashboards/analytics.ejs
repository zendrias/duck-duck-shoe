<%- include('../partials/html-head') %>
  <%- include('../partials/dashboardNav') %>


    <body>
      <h1>Anayltics</h1>
      <h3>Shoes Listed --- <%= profile.shoesListed.length %>
      </h3>
      <% let sum=0 %>
        <% let resSum=0 %>
          <div>
            <h3>Views Per Listing</h3 <p>
            <table>
              <tr>
                <th>Shoe Name</th>
                <th>Total Views</th>
              </tr>
              <% profile.shoesListed.forEach(shoe=>{ %>
                <tr>
                  <td>
                    <%= shoe.name %>
                  </td>
                  <td>
                    <%= shoe.views %>
                  </td>
                </tr>
                <% }) %>
            </table>
          </div>
          <h3>Total Listing Views ----- <%= sum %> view(s)
          </h3>
          <div class="reservations">
            <table>
              <tr>
                <th>Shoe Name</th>
                <th>Reservation(s)</th>
              </tr>
              <% profile.shoesListed.forEach(shoe=>{ %>
                <tr>
                  <td>
                    <%= shoe.name %>
                  </td>
                  <td>
                    <%= shoe.reservations %>
                  </td>
                </tr>
                <% }) %>
            </table>
          </div>
          <h3>Total Listing Reservations --- <%= resSum %>
          </h3>

    </body>

    </html>